@model List<HospitalProjectTeamThree.Models.BlogTopic>
@{
    ViewBag.Title = "List";
}

<h2>List of Blog Topics</h2>
<div class="content">
    @{
        foreach (var Topic in Model)
        {
            <div>
                <div class="detail">
                    <div>Title: @Blog.BlogTitle</div>
                    <div>Topic: @Blog.Topics </div>
                    <div>Content: @Blog.BlogContent</div>
                    <div>Source: @Blog.BlogSource </div>
                </div>

                <div><a href="/DoctorsBlog/Update/@Blog.BlogId">Edit Blog Entry</a></div>
            </div>

        }
    }
</div>
<a href="/DoctorsBlog/Add">New Blog Entry</a>