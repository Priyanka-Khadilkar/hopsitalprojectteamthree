@model List<HospitalProjectTeamThree.Models.Article>
@{
    ViewBag.Title = "ViewArticle";
}

<h2>ViewArticle</h2>
<a href="/Article/List">Back to list of all articles</a>


@{


    foreach (var Article in Model)
    {
        
        <main class="crisismain">
            <h2>@Article.ArticleTitle</h2>
            <div class="cred"><span class="auth">Author: @Article.ArticleAuthor</span><span class="publ">Published: @Article.DatePosted</span></div>
            <p>@Article.ArticleContent </p>
        </main>
    }
}
